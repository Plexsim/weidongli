            e.preventDefault();
            var imgpath = $(this).attr("data-dialogimg");
            var text = $(this).attr("data-dialogtext");
            $('.erweima_wrap').find("img").attr("src", imgpath);
            $('.erweima_wrap').find("p").text(text);
            e.stopPropagation();
            $('.erweima_wrap').show();
            return false;
        });
        //slidebar_toggle
        $('.lightApp_slidebar a').mouseover(function() {
